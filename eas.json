{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "prebuildCommand": "npm run build && npx cap copy && npx cap update",
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
